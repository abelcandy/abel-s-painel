function GuiManager:CreateTab(tabName, tabIcon)
    local TabButton = Instance.new("TextButton")
    TabButton.Name = tabName
    TabButton.Size = UDim2.new(1, 0, 0, 40)
    TabButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    TabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
    TabButton.TextSize = 12
    TabButton.Font = Enum.Font.GothamBold
    TabButton.Text = tabIcon .. " " .. tabName
    TabButton.BorderSizePixel = 0
    TabButton.Parent = self.MainGui.TabContainer
    
    -- Content Frame
    local ContentFrame = Instance.new("Frame")
    ContentFrame.Name = tabName .. "Content"
    ContentFrame.Size = UDim2.new(1, 0, 1, 0)
    ContentFrame.BackgroundTransparency = 1
    ContentFrame.Visible = false
    ContentFrame.Parent = self.MainGui.ContentContainer
    
    local ContentList = Instance.new("UIListLayout")
    ContentList.SortOrder = Enum.SortOrder.LayoutOrder
    ContentList.Padding = UDim.new(0, 10)
    ContentList.Parent = ContentFrame
    
    local Padding = Instance.new("UIPadding")
    Padding.PaddingLeft = UDim.new(0, 10)
    Padding.PaddingTop = UDim.new(0, 10)
    Padding.PaddingRight = UDim.new(0, 10)
    Padding.Parent = ContentFrame
    
    TabButton.MouseButton1Click:Connect(function()
        -- Desativar todos os abas
        for _, content in pairs(self.MainGui.ContentContainer:GetChildren()) do
            if content:IsA("Frame") and content ~= ContentFrame then
                content.Visible = false
            end
        end
        
        -- Desativar bot√µes
        for _, btn in pairs(self.MainGui.TabContainer:GetChildren()) do
            if btn:IsA("TextButton") then
                btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            end
        end
        
        -- Ativar aba atual
        ContentFrame.Visible = true
        TabButton.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    end)
    
    self.MainGui.Tabs[tabName] = {
        Button = TabButton,
        Content = ContentFrame
    }
    
    return ContentFrame
end

function GuiManager:AddToggle(parentFrame, featureName, featureIcon, onToggle)
    local ToggleFrame = Instance.new("Frame")
    ToggleFrame.Name = featureName
    ToggleFrame.Size = UDim2.new(1, 0, 0, 40)
    ToggleFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ToggleFrame.BorderSizePixel = 0
    ToggleFrame.Parent = parentFrame
    
    local Label = Instance.new("TextLabel")
    Label.Name = "Label"
    Label.Size = UDim2.new(1, -50, 1, 0)
    Label.BackgroundTransparency = 1
    Label.TextColor3 = Color3.fromRGB(200, 200, 200)
    Label.TextSize = 14
    Label.Font = Enum.Font.Gotham
    Label.Text = featureIcon .. " " .. featureName
    Label.TextXAlignment = Enum.TextXAlignment.Left
    Label.Parent = ToggleFrame
    
    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Name = "Toggle"
    ToggleButton.Size = UDim2.new(0, 40, 0, 25)
    ToggleButton.Position = UDim2.new(1, -45, 0.5, -12)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.TextSize = 12
    ToggleButton.Font = Enum.Font.GothamBold
    ToggleButton.Text = "OFF"
    ToggleButton.BorderSizePixel = 0
    ToggleButton.Parent = ToggleFrame
    
    local isEnabled = false
    
    ToggleButton.MouseButton1Click:Connect(function()
        isEnabled = not isEnabled
        
        if isEnabled then
            ToggleButton.BackgroundColor3 = Color3.fromRGB(100, 200, 100)
            ToggleButton.Text = "ON"
        else
            ToggleButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
            ToggleButton.Text = "OFF"
        end
        
        onToggle(isEnabled)
    end)
    
    return {
        Frame = ToggleFrame,
        Button = ToggleButton,
        IsEnabled = isEnabled
    }
end

function GuiManager:AddButton(parentFrame, buttonText, buttonIcon, onClick)
    local Button = Instance.new("TextButton")
    Button.Name = buttonText
    Button.Size = UDim2.new(1, 0, 0, 40)
    Button.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 14
    Button.Font = Enum.Font.GothamBold
    Button.Text = buttonIcon .. " " .. buttonText
    Button.BorderSizePixel = 0
    Button.Parent = parentFrame
    
    Button.MouseButton1Click:Connect(function()
        Button.BackgroundColor3 = Color3.fromRGB(80, 130, 200)
        wait(0.1)
        Button.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
        onClick()
    end)
    
    return Button
end

function GuiManager:BuildInterface()
    self:CreateMainPanel()
    
    -- ============ ABA 1: GR√ÅFICOS ============
    local GraphicsTab = self:CreateTab("Graphics", "üé®")
    
    self:AddToggle(GraphicsTab, "FullBright", "‚òÄÔ∏è", function(state)
        if state then
            FullBright:Enable()
        else
            FullBright:Disable()
        end
    end)
    
    self:AddToggle(GraphicsTab, "No Blur", "‚ú®", function(state)
        if state then
            NoBlur:Enable()
        else
            NoBlur:Disable()
        end
    end)
    
    self:AddToggle(GraphicsTab, "Better Water", "üíß", function(state)
        if state then
            BetterWater:Enable()
        else
            BetterWater:Disable()
        end
    end)
    
    self:AddToggle(GraphicsTab, "Potato Mode ü•î", "‚ö°", function(state)
        if state then
            PotatoMode:Enable()
        else
            PotatoMode:Disable()
        end
    end)
    
    -- ============ ABA 2: SERVIDOR ============
    local ServerTab = self:CreateTab("Server", "üñ•Ô∏è")
    
    self:AddButton(ServerTab, "Rejoin", "üîÑ", function()
        Logger:Success("Reentrando no servidor...")
        ServerModule:Rejoin()
    end)
    
    self:AddButton(ServerTab, "Server Info", "‚ÑπÔ∏è", function()
        local info = ServerModule:GetServerInfo()
        Logger:Success("Jogo: " .. info.PlaceName)
        Logger:Success("Jogadores: " .. info.PlayerCount)
        Logger:Success("ID do Servidor: " .. info.ServerId)
    end)
    
    self:AddButton(ServerTab, "List Players", "üë•", function()
        local players = ServerModule:GetPlayers()
        Logger:Success("Jogadores conectados:")
        for _, player in pairs(players) do
            print("  - " .. player.Name .. " (ID: " .. player.UserId .. ")")
        end
    end)
    
    -- ============ ABA 3: SOBRE ============
    local AboutTab = self:CreateTab("About", "‚ÑπÔ∏è")
    
    local AboutText = Instance.new("TextLabel")
    AboutText.Size = UDim2.new(1, 0, 1, 0)
    AboutText.BackgroundTransparency = 1
    AboutText.TextColor3 = Color3.fromRGB(200, 200, 200)
    AboutText.TextSize = 12
    AboutText.Font = Enum.Font.Gotham
    AboutText.Text = [[
LOW HUB v1.0.0

‚úÖ FEATURES LEG√çTIMAS:
‚Ä¢ Fullbright (Ilumina√ß√£o)
‚Ä¢ No Blur (Sem desfoque)
‚Ä¢ Better Water (√Ågua melhorada)
‚Ä¢ Potato Mode (Otimizar FPS)
‚Ä¢ Server Info (Informa√ß√µes)
‚Ä¢ Rejoin (Reentrar)

üì± Discord: discord.gg/lowhub

‚ö†Ô∏è AVISO:
Este script √© 100% leg√≠timo
e segue os TOS do Roblox.
N√£o cont√©m exploits!

Desenvolvido por: abelcandy
    ]]
    AboutText.TextWrapped = true
    AboutText.TextXAlignment = Enum.TextXAlignment.Left
    AboutText.TextYAlignment = Enum.TextYAlignment.Top
    AboutText.Parent = AboutTab
    
    -- Ativar primeira aba
    local firstTab = self.MainGui.Tabs["Graphics"]
    if firstTab then
        firstTab.Content.Visible = true
        firstTab.Button.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    end
    
    Logger:Success("Interface GUI criada com sucesso!")
end

function GuiManager:Toggle()
    if self.MainGui then
        self.IsOpen = not self.IsOpen
        self.MainGui.ScreenGui.Enabled = self.IsOpen
        
        if self.IsOpen then
            Logger:Success("Painel aberto!")
        else
            Logger:Success("Painel fechado!")
        end
    end
end

-- ============================================
-- SISTEMA DE HOTKEYS
-- ============================================

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Config.ToggleKey then
        GuiManager:Toggle()
    end
end)

-- ============================================
-- INICIALIZA√á√ÉO
-- ============================================

local function Initialize()
