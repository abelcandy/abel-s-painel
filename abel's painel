local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local UserInputService = game:GetService("UserInputService")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")

local Player = Players.LocalPlayer
local Mouse = Player:GetMouse()
local UserGui = Player:WaitForChild("PlayerGui")
local Character = Player.Character or Player.CharacterAdded:Wait()
local Camera = workspace.CurrentCamera

-- ============================================
-- CONFIGURA√á√ÉO GLOBAL
-- ============================================

local Config = {
    Version = "1.0.0",
    DiscordLink = "discord.gg/lowhub",
    ToggleKey = Enum.KeyCode.Insert, -- Tecla para abrir/fechar painel
    
    Features = {
        FullBright = false,
        PostProcessing = false,
        BetterWater = false,
        PotatoMode = false,
        NoBlur = false,
        RTXMode = false,
    },
    
    Settings = {
        Brightness = 1,
        BetterWaterIntensity = 1,
        PostProcSaturation = 1,
        FPSTarget = 60,
    }
}

-- ============================================
-- DETEC√á√ÉO DE JOGO
-- ============================================

local function DetectGame()
    local PlaceId = game.PlaceId
    local PlaceName = game.Name
    
    local GameDatabase = {
        [142823291] = "Murder Mystery 2",
        [1224901158] = "RoVibes",
        [4650681930] = "MushYO",
        [537413528] = "Adopt Me!",
        [8304191830] = "Blox Fruits",
    }
    
    return GameDatabase[PlaceId] or PlaceName
end

local CurrentGame = DetectGame()

-- ============================================
-- SISTEMA DE LOGGING
-- ============================================

local Logger = {}

function Logger:Print(message, messageType)
    messageType = messageType or "INFO"
    local prefix = "[LOW HUB - " .. messageType .. "]"
    print(prefix .. " " .. message)
end

function Logger:Success(message)
    self:Print(message, "SUCCESS")
end

function Logger:Error(message)
    self:Print(message, "ERROR")
end

function Logger:Warning(message)
    self:Print(message, "WARNING")
end

Logger:Success("Script iniciado em: " .. CurrentGame)

-- ============================================
-- M√ìDULO: FULLBRIGHT
-- ============================================

local FullBright = {}

function FullBright:Enable()
    if Config.Features.FullBright then return end
    
    -- Salvar configura√ß√µes originais
    FullBright.OriginalClockTime = Lighting.ClockTime
    FullBright.OriginalAmbient = Lighting.Ambient
    FullBright.OriginalOutdoorAmbient = Lighting.OutdoorAmbient
    
    -- Aplicar fullbright
    Lighting.ClockTime = 12
    Lighting.Ambient = Color3.fromRGB(255, 255, 255)
    Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
    
    -- Remover sombras
    for _, obj in pairs(Lighting:GetChildren()) do
        if obj:IsA("Shadow") then
            obj:Destroy()
        end
    end
    
    Config.Features.FullBright = true
    Logger:Success("FullBright ativado!")
end

function FullBright:Disable()
    if not Config.Features.FullBright then return end
    
    -- Restaurar valores originais
    Lighting.ClockTime = FullBright.OriginalClockTime or 14.5
    Lighting.Ambient = FullBright.OriginalAmbient or Color3.fromRGB(200, 200, 200)
    Lighting.OutdoorAmbient = FullBright.OriginalOutdoorAmbient or Color3.fromRGB(200, 200, 200)
    
    Config.Features.FullBright = false
    Logger:Success("FullBright desativado!")
end

function FullBright:Toggle()
    if Config.Features.FullBright then
        self:Disable()
    else
        self:Enable()
    end
end

-- ============================================
-- M√ìDULO: NO BLUR
-- ============================================

local NoBlur = {}

function NoBlur:Enable()
    if Config.Features.NoBlur then return end
    
    -- Remover motion blur da c√¢mera
    local Camera = workspace.CurrentCamera
    
    if Camera:FindFirstChild("MotionBlur") then
        Camera.MotionBlur:Destroy()
    end
    
    -- Adicionar observador para novos efeitos
    NoBlur.Connection = Lighting.ChildAdded:Connect(function(child)
        if child:IsA("PostProcessEffect") then
            if child.Name == "MotionBlur" or child:FindFirstChild("Blur") then
                child:Destroy()
            end
        end
    end)
    
    Config.Features.NoBlur = true
    Logger:Success("No Blur ativado!")
end

function NoBlur:Disable()
    if not Config.Features.NoBlur then return end
    
    if NoBlur.Connection then
        NoBlur.Connection:Disconnect()
    end
    
    Config.Features.NoBlur = false
    Logger:Success("No Blur desativado!")
end

function NoBlur:Toggle()
    if Config.Features.NoBlur then
        self:Disable()
    else
        self:Enable()
    end
end

-- ============================================
-- M√ìDULO: POTATO MODE (Otimiza√ß√£o FPS)
-- ============================================

local PotatoMode = {}

function PotatoMode:Enable()
    if Config.Features.PotatoMode then return end
    
    -- Reduzir qualidade de texturas
    for _, part in pairs(workspace:FindPartBoundsInRadius(Player.Character.PrimaryPart.Position, 500)) do
        if part:IsA("Part") or part:IsA("MeshPart") then
            part.Material = Enum.Material.Plastic
            if part:FindFirstChild("Texture") then
                part.Texture:Destroy()
            end
        end
    end
    
    -- Desabilitar sombras
    Lighting.GlobalShadows = false
    
    -- Reduzir render distance
    for _, obj in pairs(Lighting:GetChildren()) do
        if obj:IsA("Atmosphere") then
            obj.Density = 0.1
        end
    end
    
    -- Desabilitar efeitos de part√≠culas
    for _, emitter in pairs(workspace:FindPartBoundsInRadius(Player.Character.PrimaryPart.Position, 500)) do
        for _, child in pairs(emitter:GetChildren()) do
            if child:IsA("ParticleEmitter") then
                child.Enabled = false
            end
        end
    end
    
    Config.Features.PotatoMode = true
    Logger:Success("Potato Mode (FPS Boost) ativado!")
end

function PotatoMode:Disable()
    if not Config.Features.PotatoMode then return end
    
    Lighting.GlobalShadows = true
    
    for _, emitter in pairs(workspace:FindPartBoundsInRadius(Player.Character.PrimaryPart.Position, 500)) do
        for _, child in pairs(emitter:GetChildren()) do
            if child:IsA("ParticleEmitter") then
                child.Enabled = true
            end
        end
    end
    
    Config.Features.PotatoMode = false
    Logger:Success("Potato Mode desativado!")
end

function PotatoMode:Toggle()
    if Config.Features.PotatoMode then
        self:Disable()
    else
        self:Enable()
    end
end

-- ============================================
-- M√ìDULO: BETTER WATER
-- ============================================

local BetterWater = {}

function BetterWater:Enable()
    if Config.Features.BetterWater then return end
    
    -- Encontrar terreno de √°gua
    local Terrain = workspace.Terrain
    
    -- Melhorar reflexo da √°gua (simulado via c√¢mera)
    local Camera = workspace.CurrentCamera
    
    -- Aumentar brilho de reflexos
    Lighting.Ambient = Lighting.Ambient + Color3.fromRGB(10, 10, 10)
    
    Config.Features.BetterWater = true
    Logger:Success("Better Water ativado!")
end

function BetterWater:Disable()
    if not Config.Features.BetterWater then return end
    
    Config.Features.BetterWater = false
    Logger:Success("Better Water desativado!")
end

function BetterWater:Toggle()
    if Config.Features.BetterWater then
        self:Disable()
    else
        self:Enable()
    end
end

-- ============================================
-- M√ìDULO: SERVIDOR
-- ============================================

local ServerModule = {}

function ServerModule:GetPlayers()
    local playerList = {}
    for _, player in pairs(Players:GetPlayers()) do
        table.insert(playerList, {
            Name = player.Name,
            UserId = player.UserId,
            Character = player.Character ~= nil
        })
    end
    return playerList
end

function ServerModule:GetServerInfo()
    return {
        PlaceId = game.PlaceId,
        PlaceName = game.Name,
        PlayerCount = #Players:GetPlayers(),
        MaxPlayers = game:FindFirstChild("MaxPlayers") and game.MaxPlayers or "Desconhecido",
        ServerId = game.JobId
    }
end

function ServerModule:Rejoin()
    local PlaceId = game.PlaceId
    TeleportService:Teleport(PlaceId, Player)
    Logger:Success("Rejoin iniciado...")
end

function ServerModule:GetServerList()
    -- Simula√ß√£o - em um cen√°rio real, usaria APIs do Roblox
    local info = self:GetServerInfo()
    return {
        {
            Name = "Servidor Atual",
            Players = info.PlayerCount,
            JobId = info.ServerId,
            IsCurrent = true
        }
    }
end

