-- Aguardar renderiza√ß√£o
wait(0.5)
Initialize()

-- ============================================
-- LIMPEZA AO SAIR
-- ============================================

Players.PlayerRemoving:Connect(function(player)
    if player == Player then
        if GuiManager.MainGui then
            GuiManager.MainGui.ScreenGui:Destroy()
        end
        Logger:Success("Script finalizado")
    end
end)
